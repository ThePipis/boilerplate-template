generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/**
 * BOILERPLATE: Modelo de ejemplo.
 * - Puedes eliminar o renombrar `Example` cuando definas tu dominio real.
 */
model Example {
  id        String   @id @default(uuid())
  label     String
  createdAt DateTime @default(now())
}
